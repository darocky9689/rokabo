{
  "name": "rokabo-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "preview:local": "npm run dev",
    "build": "next build",
    "build:dist": "rm -rf dist-site && npm run build && mv out dist-site",
    "release:check": "npm run build:dist",
    "release:live": "bash scripts/release-live.sh",
    "release:live:seo": "RUN_SEO_AUDIT=1 bash scripts/release-live.sh",
    "check:prepush": "npm run build:dist",
    "seo:audit": "npm run build:dist && node scripts/seo-content-audit.mjs website && node scripts/seo-broken-links.mjs && node scripts/seo-image-audit.mjs",
    "seo:content": "node scripts/seo-content-audit.mjs",
    "seo:links": "node scripts/seo-broken-links.mjs",
    "seo:images": "node scripts/seo-image-audit.mjs",
    "deploy:no-onedrive": "bash scripts/deploy-without-onedrive.sh",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "14.2.32",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "20.17.19",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.32",
    "typescript": "5.8.2"
  }
}